        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.io.*;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());


	String rootPath = getVariable("PATH", "");
	String state = getVariable("STATE", "");
	String buyer = getVariable("BUYER", "");
	String doctype = getVariable("DOCTYPE", "");
	String supplierId = get(Fields.In, "receivermailboxid").getString(r);

	
	String fullPath = rootPath + "/" + buyer + "/" + doctype + "/" + state + "/" + supplierId + "/";

	String fileName = get(Fields.In, "doc_id").getString(r) + "REJECT";

	String filePathR = fullPath + fileName;

	File tempFile = new File(fullPath);
	tempFile.mkdirs();
	// Send the row on to the next step.
	get(Fields.Out, "FILEPATHR").setValue(r, filePathR);
    putRow(data.outputRowMeta, r);

	return true;
}
</class_source>
