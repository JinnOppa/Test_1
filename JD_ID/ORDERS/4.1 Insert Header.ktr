        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    
	r = createOutputRow(r, data.outputRowMeta.size());

	String split_dc_code = get(Fields.In, "split_dc_code").getString(r);
	String receiver = get(Fields.In, "RECEIVERMAILBOXID").getString(r);
	String supplier = "";

	if(receiver.equals("UNILEVER_ID") && split_dc_code.equals("102"))
		supplier = "UNILEVER_JD_KALIMANTAN";
	else supplier = "UNILEVER_JD_ID";
	
	get(Fields.Out, "supplierorgid").setValue(r, supplier);
	

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
