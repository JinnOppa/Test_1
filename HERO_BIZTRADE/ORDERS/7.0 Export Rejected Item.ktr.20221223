	String fileName = get(Fields.In, "doc_id").getString(r) + "REJECT";

	String filePathR = fullPath + fileName;

	File tempFile = new File(fullPath);
	tempFile.mkdirs();
	// Send the row on to the next step.
	get(Fields.Out, "FILEPATHR").setValue(r, filePathR);
    putRow(data.outputRowMeta, r);

	return true;
}
