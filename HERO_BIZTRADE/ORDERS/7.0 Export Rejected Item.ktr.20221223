    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
	
	Long checkItemPrice = get(Fields.In, "check_item_price").getInteger(r);
	Double calcCaseCrice = get(Fields.In, "unit_price").getNumber(r); // po case price
	Double netUnitPrice = get(Fields.In, "gross_unit_price").getNumber(r);   // po unit price

	Double unitPrice = get(Fields.In, "im_unitprice").getNumber(r); // item master case price
	Double listPrice = get(Fields.In, "list_price").getNumber(r); // item master unit price

	Double buyerPrice = 0.0000;
	Double supplierPrice = 0.0000;

	if (checkItemPrice == 1 ) {
		buyerPrice = calcCaseCrice;
		supplierPrice = unitPrice;
	}
	
	if (checkItemPrice == 2 || checkItemPrice == 0){
		buyerPrice = netUnitPrice;
		supplierPrice = listPrice;
	}

	if (checkItemPrice == 3){
		buyerPrice = netUnitPrice;
		supplierPrice = unitPrice;
	}

	get(Fields.Out, "Buyer Unit Price").setValue(r, buyerPrice);
	get(Fields.Out, "Supplier Unit Price").setValue(r, supplierPrice);
	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
