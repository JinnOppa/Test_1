    <sql>SELECT distinct d.doc_id
, h.po_id
, d.item_no
, im.use_standard_format
, im.check_item_uom
, im.export_invalid_item
FROM hero_biztrade_orders_header h 
INNER JOIN im_orders_config im ON h.sendermailboxid=im.buyer_id AND h.receivermailboxid=im.supplier_id 
INNER JOIN hero_biztrade_orders_detail d ON h.doc_id=d.doc_id 
WHERE im.doctype='ORDERS' AND im.use_standard_format=0 AND sendermailboxid='HERO_INDO' AND receivermailboxid='NESTLE_INDO' AND ((im.export_invalid_item=0 AND d.validation_result=0) OR im.export_invalid_item=1)
ORDER BY h.po_id, d.line_item_ref_no
</sql>
