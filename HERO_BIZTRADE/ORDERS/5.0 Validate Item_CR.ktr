        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    
	r = createOutputRow(r, data.outputRowMeta.size());

	long new_validation_result=0;

	// setting for checking
	Long checkItemAvailability = get(Fields.In, "check_item_availability").getInteger(r);
	Long checkItemBarcode = get(Fields.In, "check_item_barcode").getInteger(r);
	Long checkItemPrice = get(Fields.In, "check_item_price").getInteger(r);
	Long checkItemUom = get(Fields.In, "check_item_uom").getInteger(r);
	Long checkItemDesc = get(Fields.In, "check_item_desc").getInteger(r);
	Long checkSupplierItemCode = get(Fields.In, "check_supplier_item_code").getInteger(r);
	Long checkPODeliveryDate = get(Fields.In, "check_po_delivery_date").getInteger(r);

	//PO info
    Double unitPrice = get(Fields.In, "unit_price").getNumber(r);
	String po_type = get(Fields.In, "n_po_type").getString(r);
	Long qty_case = get(Fields.In, "qty_case").getInteger(r);
	String supplierItemCode = get(Fields.In, "supplier_item_code").getString(r);
	String description = get(Fields.In, "item_description").getString(r);
	Long differenceDays = get(Fields.In, "days").getInteger(r);
	Double maxPrice = get(Fields.In, "maxPrice").getNumber(r);
    Double minPrice = get(Fields.In, "minPrice").getNumber(r);

	// item master info
	String po_id = get(Fields.In, "po_id").getString(r);
	Long product_status = get(Fields.In, "item_status").getInteger(r);
	Double price = get(Fields.In, "list_price").getNumber(r);
	Long qty = get(Fields.In, "total_qty").getInteger(r);
	String consumerUnitEan = get(Fields.In, "consumer_unit_ean").getString(r);
    Double unit_price_im = get(Fields.In, "unit_price_im").getNumber(r);


	if(checkItemAvailability==0 &amp;&amp; checkItemBarcode==0 &amp;&amp; checkItemDesc==0 &amp;&amp;
		checkSupplierItemCode==0 &amp;&amp; checkItemPrice==0 &amp;&amp; checkItemUom==0)
	{
		new_validation_result = new_validation_result | 0;
	}
	else
		{
			if((consumerUnitEan==null || consumerUnitEan.trim().equals("")) &amp;&amp; checkItemBarcode==1){
				 new_validation_result = new_validation_result | 1;
			}
			else if(product_status!=null &amp;&amp; product_status==0 &amp;&amp; checkItemAvailability==1){
				 new_validation_result = new_validation_result | 2;
			}
			else if ((description==null || description.trim().equals("")) &amp;&amp; checkItemDesc==1){
				 new_validation_result = new_validation_result | 4;
			}
			else if ((supplierItemCode==null || supplierItemCode.trim().equals("")) &amp;&amp; checkSupplierItemCode==1){
					new_validation_result = new_validation_result | 8;
			}
			else if ((price > unitPrice) &amp;&amp; price != unitPrice){
					new_validation_result = new_validation_result | 16;
			}
	



			/*
			else if ((price &lt; unitPrice) &amp;&amp; price != unitPrice){
					new_validation_result = new_validation_result | 32;
			}*/

			/*
			if ((qty > qty_case) &amp;&amp; qty != qty_case)
					new_validation_result = new_validation_result | 16;

			if ((qty &lt; qty_case) &amp;&amp; qty != qty_case)
					new_validation_result = new_validation_result | 32;
			*/
		
			//checkPoDeliveryDate
			if (checkPODeliveryDate == 1){
			   if(differenceDays &lt; 2 ){	   
					new_validation_result = new_validation_result | 64;
				}
			}

		    if(checkItemPrice > 0){
				if(checkItemPrice==1){
					if (unit_price_im > maxPrice)
							new_validation_result = new_validation_result | 128;
					if (unit_price_im &lt; minPrice)
							new_validation_result = new_validation_result | 128;
				}
			}

				
		
	}
	get(Fields.Out, "new_validation_result").setValue(r, new_validation_result);
	

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
