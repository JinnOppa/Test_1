    <connection>im_staging</connection>
    <sql>SELECT 
distinct d.doc_id
, h.po_id
, im.doctype
, d.item_no
, d.item_barcode
FROM hero_biztrade_orders_header h 
INNER JOIN im_orders_config im ON h.sendermailboxid=im.buyer_id AND h.receivermailboxid=im.supplier_id 
