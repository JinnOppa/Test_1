    INNER JOIN biztrade_hero_id.order_party buyerop ON buyerop.id=po.buyer_id
    INNER JOIN biztrade_hero_id.order_party sellerop ON sellerop.id=po.seller_id
    INNER JOIN biztrade_hero_id.order_party shiptoop ON shiptoop.id=po.ship_to_id
    INNER JOIN biztrade_hero_id.order_party warehouseop ON warehouseop.id=po.warehouse_id
    INNER JOIN affiliate aff ON aff.id=opi.counter_party_id
