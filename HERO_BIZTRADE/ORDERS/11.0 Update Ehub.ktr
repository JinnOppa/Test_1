import javax.activation.DataHandler;
import org.apache.commons.codec.binary.Base64;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
	String CONTENT = get(Fields.In, "File content").getString(r);
	 //encoding  byte array into base 64
	byte[] b = Base64.encodeBase64(CONTENT.getBytes());
	String collectionName = getVariable("COLLECTIONNAME", "");
	String systemName = getVariable("SYSTEMNAME", "");

	get(Fields.Out, "collectionName").setValue(r, collectionName);
	get(Fields.Out, "systemName").setValue(r, systemName);	
	get(Fields.Out,"data").setValue(r,b);

	/* TODO: Your code here. (See Sample)
    
