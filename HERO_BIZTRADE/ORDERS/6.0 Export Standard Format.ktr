    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
	
	String hSupplierLocationName = get(Fields.In, "h_supplier_location_name").getString(r);
	String hDtLocationName = get(Fields.In, "h_dt_location_name").getString(r);

	String deliveryTime="00:00";
	String orderType="INITIAL";

	get(Fields.Out, "supplier_location_name").setValue(r, hSupplierLocationName);
	get(Fields.Out, "delivery_to_location").setValue(r, hDtLocationName);
	get(Fields.Out, "delivery_time").setValue(r, deliveryTime);
	get(Fields.Out, "order_type").setValue(r, orderType);
 
	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
</class_source>
      </definition>
